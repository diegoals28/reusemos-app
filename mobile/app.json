{
  "expo": {
    "name": "Reusemos",
    "slug": "reusemos",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "reusemos",
    "owner": "diegoals28",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2D9B6E"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.reusemos.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "Reusemos necesita acceso a tu cámara para tomar fotos de tus productos.",
        "NSPhotoLibraryUsageDescription": "Reusemos necesita acceso a tus fotos para publicar productos.",
        "NSLocationWhenInUseUsageDescription": "Reusemos necesita tu ubicación para mostrarte productos cercanos.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2D9B6E"
      },
      "package": "com.reusemos.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.RECORD_AUDIO"
      ],
      "usesCleartextTraffic": true
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-camera",
      "expo-image-picker",
      "expo-location",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#2D9B6E",
          "sounds": []
        }
      ],
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "Reusemos necesita acceso a tus fotos para publicar productos.",
          "cameraPermission": "Reusemos necesita acceso a tu cámara para tomar fotos de tus productos."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2032308a-eb02-4859-97e4-b71c45afc531"
      }
    },
    "runtimeVersion": "1.0.0"
  }
}
